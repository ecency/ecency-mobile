+
+  override fun exec() {
+    wipeOutputDir()
+    setupCommandLine()
+    super.exec()
+  }
+
+  internal fun wipeOutputDir() {
+    generatedSrcDir.asFile.get().apply {
+      deleteRecursively()
+      mkdirs()
+    }
+  }
+
+  internal fun setupCommandLine() {
+    val workingDir = File(nodeWorkingDir.get())
