+    assertThat(ModelAutolinkingDependenciesJson("", "react(native", null).nameCleansed)
+        .isEqualTo("react_native")
+    assertThat(ModelAutolinkingDependenciesJson("", "react)native", null).nameCleansed)
+        .isEqualTo("react_native")
