+     */
+    internal fun checkAndUpdateLockfiles(lockFiles: FileCollection, outputFolder: File): Boolean {
+      var changed = false
+      lockFiles.forEach { lockFile ->
+        if (lockFile.exists()) {
+          val sha = computeSha256(lockFile)
+          val shaFile = File(outputFolder, "${lockFile.name}.sha")
